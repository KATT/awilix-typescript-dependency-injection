    "url": "https://github.com/KATT/awilix-ts-test/issues"
  },
  "homepage": "https://github.com/KATT/awilix-ts-test#readme",
  "dependencies": {
    "@types/node": "^10.0.3",
    "awilix": "^3.0.7",
    "yarn": "^1.6.0"
  },
  "devDependencies": {
    "nodemon": "^1.17.3",
    "rimraf": "^2.6.2",
    "ts-node": "^6.0.2",
    "typescript": "^2.8.3"
  }
}
