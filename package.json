{
  "name": "awilix-ts-test",
  "version": "1.0.0",
  "private": true,
  "description": "playing around with awilix and typescript",
  "main": "dist/index.js",
  "scripts": {
    "build": "rimraf dist && tsc -d",
    "dev": "nodemon --watch src --exec ts-node src/index.ts",
    "start": "node dist",
    "test": "yarn build && yarn start",
    "test:watch": "nodemon --watch src --exec yarn test"
  },
